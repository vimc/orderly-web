{

  "id": "UserGroups",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "members": {
        "type": "array",
        "items": {
          "$ref": "User.schema.json"
        }
      },
      "permissions": {
        "type": "array",
        "items": {
          "$ref": "Permission.schema.json"
        }
      }
    },
    "additionalProperties": false,
    "required": ["name", "members"]
  }
}